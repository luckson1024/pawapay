{
    "name": "myzuwa/pawapay-sdk",
    "description": "PawaPay SDK for Myzuwa.com",
    "type": "library",
    "version": "1.0.0",
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.0",
        "ext-json": "*",
        "monolog/monolog": "^2.0",
        "psr/log": "^1.1",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Myzuwa\\PawaPay\\": "src/"
        },
        "files": [
            "src/Support/Helpers.php",
            "config/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Myzuwa\\PawaPay\\Tests\\": "tests/",
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
